{
  "expo": {
    "name": "MyFitWave",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "jsEngine": "hermes",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png",
      "name": "MyFitWave",
      "shortName": "MyFitWave",
      "lang": "en",
      "scope": "/",
      "display": "standalone",
      "startUrl": "/",
      "orientation": "portrait",
      "description": "Your personal fitness companion",
      "dir": "ltr",
      "preferRelatedApplications": false,
      "themeColor": "#2B86C5",
      "backgroundColor": "#ffffff",
      "build": {
        "babel": {
          "include": [
            "@expo/vector-icons"
          ]
        }
      },
      "pwa": {
        "name": "MyFitWave",
        "shortName": "MyFitWave",
        "description": "Your personal fitness companion",
        "startUrl": "/",
        "display": "standalone",
        "backgroundColor": "#ffffff",
        "themeColor": "#2B86C5",
        "orientation": "portrait",
        "icons": [
          {
            "src": "./assets/images/icon-192x192.png",
            "sizes": "192x192",
            "type": "image/png",
            "purpose": "any maskable"
          },
          {
            "src": "./assets/images/icon-512x512.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "any maskable"
          },
          {
            "src": "./assets/images/icon-1024x1024.png",
            "sizes": "1024x1024",
            "type": "image/png",
            "purpose": "any maskable"
          }
        ]
      }
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    }
  }
}
